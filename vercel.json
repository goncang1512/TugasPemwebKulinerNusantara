{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "php"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/assets/*"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "build": {
    "env": {
      "COMPOSER_CACHE_DIR": ".vercel/cache/composer"
    }
  }
}
